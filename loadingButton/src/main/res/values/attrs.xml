<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingButton">
        <attr name="running_text" format="string"/>
        <attr name="final_text" format="string"/>
        <attr name="final_background" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MorphLoadingButton">
        <attr name="initial_corner_radius" format="dimension"/>
        <attr name="final_corner_radius" format="dimension"/>
        <attr name="final_width" format="dimension"/>
        <attr name="final_height" format="dimension"/>
    </declare-styleable>

</resources>